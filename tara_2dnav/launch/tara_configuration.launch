<?xml version="1.0"?>

<launch>
	<include file="$(find tara_2dnav)/launch/camera.launch"/>
	
	<node pkg="$(find tara_2dnav)" type="init_connection" name="init_connection" output="screen"/>
		<param name="ip" value="10.42.0.1"/>
		<param name="user" value="ubuntu"/>

	<!-- Tf from base_link -> camera_link-->
	<node pkg="tara_firmware" type="tf_broadcaster" name="tara_tf_publisher" output="screen"> </node>
</launch>
